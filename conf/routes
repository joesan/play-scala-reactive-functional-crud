# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# TEST PAGE
GET			/							com.inland24.crud.controllers.MyApplicationController.home

# MyObservable
GET			/observable					com.inland24.crud.controllers.MyApplicationController.observable

# MyConnectableObservable
GET			/connectableObservable		com.inland24.crud.controllers.MyApplicationController.connectableObservable

# Map static resources from the /public folder to the /assets URL path
GET			/assets/*file				controllers.Assets.versioned(path="/public", file: Asset)